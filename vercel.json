{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "headers": [
        {
            "source": "/admin/config.yml",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/yaml"
                }
            ]
        }
    ]
}
